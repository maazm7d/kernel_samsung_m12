			acpm_mfd_bus@11A50000 {
				status = "okay";
				#address-cells = <1>;
				#size-cells = <0>;
				interrupt-parent = <&gic>;
				codec_aud3004x: aud3004x@07 {
					compatible = "codec,aud3004x";
					#sound-dai-cells = <0>;
					reg = <0x07>;
					i2c-speedy-address;
					pinctrl-names = "default";
					io-channels = <&exynos_adc 7>,<&exynos_adc 6>;
					io-channel-names = "adc-gdet","adc-ear";
					#io-channel-cells = <1>;
					ldet-vth = <2>;
					ldet-pop = <0>;
					mic-adc-range = <1407>;
					adc-thd-fake-jack = <101>;
					adc-thd-auxcable = <201>;
					adc-thd-water-in = <601>;
					adc-thd-water-out = <3151>;
					but-zones-list = <&but_zones 226 0 377>,
						<&but_zones 582 378 449>,
						<&but_zones 115 450 628>,
						<&but_zones 114 629 1161>;
					btn-release-value = <1407>;
					jackout-dbnc-time = <2>;
				};
			}; /* end of acpm_mfd_bus@11a10000 */

			usb_hs_tune:hs_tune {
				status = "disabled";
				hs_tune_cnt = <8>;
				/* value = <device host> */
				hs_tune1 {
					tune_name = "compdis";
					tune_value = <0x7 0x7>;
				};

				hs_tune2 {
					tune_name = "tx_pre_emp";
					tune_value = <0x3 0x3>;
				};

				hs_tune3 {
					tune_name = "tx_pre_emp_plus";
					tune_value = <0x0 0x0>;
				};

				hs_tune4 {
					tune_name = "tx_res";
					tune_value = <0x3 0x3>;
				};

				hs_tune5 {
					tune_name = "tx_rise";
					tune_value = <0x3 0x3>;
				};

				hs_tune6 {
					tune_name = "tx_vref";
					tune_value = <0xe 0xc>;
				};

				hs_tune7 {
					tune_name = "rx_sqrx";
					tune_value = <0x7 0x7>;
				};

				hs_tune8 {
					tune_name = "utmi_clk";
					tune_value = <0x1 0x1>;
				};
			};

