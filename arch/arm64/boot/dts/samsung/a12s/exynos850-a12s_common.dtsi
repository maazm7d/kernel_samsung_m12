				compatible = "samsung,usb-notifier";
				udc = <&udc>;
				disable_host_wakelock;
			};

			fm@14AC0000 {
				without_elna = <1>;
				num-volume-level = <16>;
				val-vol-level = < 0 16 23 32 45 64 90 128 181 256 362 512 724 1024 1447 1721 >;
			};
		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */

&pinctrl_0 {
	dwmmc2_cd_ext_irq: dwmmc2_cd_ext_irq {
